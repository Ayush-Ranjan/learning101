def print_formatted(number):
    # your code goes here
    l=len(bin(number).replace("0b",""))
    for x in range(1,number+1):
        a=hex(x).replace("0x","")
        for y in range(len(a)):
            ch=a[y]
            if(ord(ch)>=97):
                a=a[:y]+chr(ord(ch)-32)+a[y+1:]

        print(str(x).rjust(l),oct(x).replace("0o","").rjust(l),a.rjust(l),bin(x).replace("0b","").rjust(l),sep=' ')

if __name__ == '__main__':
    n = int(input())
    print_formatted(n)