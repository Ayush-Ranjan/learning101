def count_substring(string, sub_string):
    c=0
    end=0;
    while end<len(string):
        end=string.find(sub_string,end,len(string));
        #print(end)
        if(end==-1):
            break
        end=end+1    
        c=c+1
    return c

if __name__ == '__main__':
    string = input().strip()
    sub_string = input().strip()
    
    count = count_substring(string, sub_string)
    print(count)